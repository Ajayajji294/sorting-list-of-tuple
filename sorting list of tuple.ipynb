   "source": [
    "x= [(2, 5), (1, 2), (4, 4), (2, 3), (2, 1)]\n",
    "for i in range(len(x)):\n",
    "    for j in range(len(x)-i-1):\n",
    "        if (x[j][1]>x[j+1][1]):\n",
    "            next=x[j]\n",
    "            x[j]=x[j+1]\n",
    "            x[j+1]=next\n",
    "print(x)\n",
    "            "
